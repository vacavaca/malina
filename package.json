{
  "name": "malina",
  "version": "0.0.4",
  "description": "Templating and state management tool",
  "main": "dist/index.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -f dist/index.*.js",
    "build:bundle": "rollup -c rollup.config.js",
    "build:uglify": "uglifyjs -m toplevel dist/index.src.js > dist/index.min.js",
    "build:clean": "rm -f dist/index.src.js",
    "build": "npm rum clean && npm run build:bundle && npm run build:uglify && npm run build:clean"
  },
  "keywords": [
    "ui",
    "jsx",
    "template",
    "hyperscript",
    "virtualdom",
    "dom",
    "vdom",
    "state",
    "flux",
    "dom",
    "html"
  ],
  "author": "vacavaca <vacavaca@fastmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vacavaca/malina.git"
  },
  "bugs": {
    "url": "https://github.com/vacavaca/malina/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "uglify-js": "^3.4.9"
  }
}
